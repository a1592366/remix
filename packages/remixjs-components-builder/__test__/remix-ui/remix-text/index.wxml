
<template name="text">
<text 
  child="{{child}}"
	innerText="{{innerText}}"
	id="{{uuid}}"
	style="{{style}}"
	class="{{className}}"
	selectable="{{selectable}}"
	space="{{space}}"
	decode="{{decode}}"
>
  
  <block wx:if="{{innerText}}">{{innerText}}</block>
  <block wx:elif="{{child}}"><view child="{{child}}" /></block>
  
</text>
</template>  







