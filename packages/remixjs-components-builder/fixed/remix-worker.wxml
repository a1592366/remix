<template name="selection">
  <block wx:if="{{ element.tagName == '#text' }}">{{element.text}}</block>
  <%- components %>
</template>

<template name="remix-worker">  
  <template is="selection" data="{{ element }}" />
  <block wx:if="{{element.slibing}}">
    <template is="selection" data="{{ element: element.slibing }}" />
  </block>
</template>