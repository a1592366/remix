<import src="../remix-worker.wxml" /> 
<view child="{{child}}" innerText="{{innerText}}" id="{{uuid}}" style="{{styles}}" class="{{className}}" hover-class="{{hoverClass}}" hover-stop-propagation="{{hoverStopPropagation}}" hover-start-time="{{hoverStartTime}}" hover-stay-time="{{hoverStayTime}}"><block wx:if="{{innerText}}">{{innerText}}</block><block wx:elif="{{child}}"><template is="remix-worker" data="{{ element: child }}" /></block>
</view>
