<import src="../remix-worker.wxml" /> 
<swiper-item bindtouchstart="{{onTouchStart}}" bindtouchmove="{{onTouchMove}}" bindtouchcancel="{{onTouchCancel}}" bindtouchend="{{onTouchEnd}}" bindtap="{{onTap}}" bindlongpress="{{onLongPress}}" bindlongtap="{{onLongTap}}" bindtransitionend="{{onTransitionEnd}}" bindanimationstart="{{onAnimationStart}}" bindanimationiteration="{{onAnimationIteration}}" bindanimationend="{{onAnimationEnd}}" bindtouchforcechange="{{onTouchForceChange}}" child="{{child}}" innerText="{{innerText}}" id="{{uuid}}" style="{{styles}}" class="{{className}}" item-id="{{itemId}}"><block wx:if="{{innerText}}">{{innerText}}</block><block wx:elif="{{child}}"><template is="remix-worker" data="{{ element: child }}" /></block>
</swiper-item>
